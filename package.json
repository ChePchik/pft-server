{
    "name": "PFT",
    "version": "2.0.5",
    "description": "Platform for Test, server",
    "main": "server.js",
    "homepage": "https://pft-server.herokuapp.com/",
    "author": "CheK",
    "scripts": {
        "install:client": "npm install --prefix client",
        "install:server": "npm install --prefix server",
        "start": "node server.js",
        "server": "nodemon server.js",
        "client": "npm run dev --prefix client",
        "dev": "concurrently \"npm run server\" \"npm run client\""
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "concurrently": "^5.3.0",
        "express": "^4.17.1",
        "is-empty": "^1.2.0",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.8.4",
        "swagger-jsdoc": "^5.0.1",
        "swagger-ui-express": "^4.1.6",
        "validator": "^12.1.0"
    },
    "devDependencies": {
        "dotenv": "^8.2.0",
        "nodemon": "^2.0.2"
    }
}
